{% extends '::base.html.twig' %}

{% block body -%}
    <h1>SocialNetwork</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Fblink</th>
                <td>{{ entity.fbLink }}</td>
            </tr>
            <tr>
                <th>Twitterlink</th>
                <td>{{ entity.twitterLink }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('socialnetwork') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('socialnetwork_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
